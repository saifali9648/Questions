#include<iostream>
using namespace std;
int main()
{
	int n;
	cout<<"enter a number:-"<<endl;
	cin>>n;
	int nop;
	cout<<"enter the no of note yout wnt"<<endl;
	cin>>nop;
	int fifty,twenty,one;
	switch(nop)
	{
		case 100:cout<<(n/100)<<endl;
			break;
		case 50:  fifty=n%100;
				cout<<(fifty/50)<<endl;
		//case 50: cout<<(n/50(n%100))<<endl;
			break;
		case 20: twenty=(n%100);
				cout<<(twenty/20)<<endl;
			break;
		//case 10:ten=(n%100);
		//	break;
		case 1:one=(n%100);
				one=(one%20);
				cout<<(one/1);
			break;
		default: cout<<"invalid choise"<<endl;
			break;
	}
return 0;
}
